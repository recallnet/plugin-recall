{
  "name": "@elizaos-plugins/plugin-recall",
  "version": "0.0.1",
  "license": "MIT AND Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Recall Network plugin for ElizaOS.",
  "repository": "https://github.com/recallnet/plugin-recall.git",
  "homepage": "https://github.com/recallnet/plugin-recall#readme",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "dependencies": {
    "@dsnp/parquetjs": "^1.8.5",
    "@elizaos/core": "^0.25.9",
    "@recallnet/chains": "^0.0.5",
    "@recallnet/sdk": "^0.0.6",
    "duckdb": "^1.2.0",
    "tsup": "8.3.5",
    "uuid": "^11.1.0",
    "viem": "^2.23.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "prettier": "^3.5.3",
    "tsup": "8.3.5",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  },
  "agentConfig": {
    "pluginType": "elizaos:client:1.0.0",
    "pluginParameters": {
      "RECALL_PRIVATE_KEY": {
        "type": "string",
        "minLength": 1,
        "description": "Recall private key for authentication"
      },
      "RECALL_BUCKET_ALIAS": {
        "type": "string",
        "minLength": 1,
        "description": "Recall bucket alias is required"
      },
      "RECALL_COT_LOG_PREFIX": {
        "type": "string",
        "minLength": 1,
        "description": "Recall COT log prefix is required"
      },
      "RECALL_NETWORK": {
        "type": "string",
        "description": "Recall network is optional"
      },
      "RECALL_SYNC_INTERVAL": {
        "type": "number",
        "description": "Recall sync interval is optional"
      },
      "RECALL_BATCH_SIZE": {
        "type": "number",
        "description": "Recall batch size is optional"
      }
    }
  }
}
